    set /p to_kill=Nom du programme √† fermer (ex: notepad.exe) ^>
    if not "%to_kill%"=="" (
        taskkill /f /im "%to_kill%" >nul 2>&1
        echo ‚úÖ Tentative de fermeture de %to_kill%
