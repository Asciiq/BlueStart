    taskkill /f /im discord.exe >nul 2>&1
    echo ‚úÖ Discord ferm√©.
    pause
    goto process_tools
)

if "%proc_choice%"=="4" (
    taskkill /f /im steam.exe >nul 2>&1
    echo ‚úÖ Steam ferm√©.
    pause
    goto process_tools
)

if "%proc_choice%"=="5" (
    taskkill /f /im chrome.exe >nul 2>&1
